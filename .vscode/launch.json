    "version": "0.2.0",
    "configurations": [
        {
            "name": "No date",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Valid date",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "1996-08-21"
        },
        {
            "name": "Past date",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "1800-08-21"
        },
        {
            "name": "Future date",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "2025-08-21"
        },
        {
            "name": "Wrong date format",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "12-08-2021"
        }
    ]
}